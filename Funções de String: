b)

SELECT UPPERCASE(nome) AS nome_maiusculo FROM nomes;

c)

SELECT nome, LENGTHS(nome) AS tamanho_nome FROM nomes;

d)

SELECT 
    CONCAT('Sr. ', nome) AS cumprimento
FROM nomes;
