a) 

CREATE TABLE eventos (
    data_evento DATE
);

INSERT INTO eventos (data_evento) VALUES
    ('15-10-2023'),
    (NOW()),
    ('2023-10-30');

b)

INSERT INTO eventos (data_evento) VALUES (CURRENT_TIMESTAMP);

c)

SELECT DATEDIFFERENCA('2023-10-30', '2023-10-15') AS dias_entre_datas;

d)

SELECT data_evento, DIA_SEMANA(data_evento) AS nome_dia_semana FROM eventos;
